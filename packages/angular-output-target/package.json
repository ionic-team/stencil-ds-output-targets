{
  "name": "@stencil/angular-output-target",
  "version": "0.0.0",
  "description": "This is an output plugin for stencil.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "resources/"
  ],
  "scripts": {
    "prebuild": "rimraf ./dist && npm run test && npm run lint",
    "build": "tsc && npm run rollup",
    "watch": "tsc --watch",
    "rollup": "rollup -c",
    "version": "npm run build",
    "release": "np",
    "lint": "tslint --project tsconfig.json",
    "test": "jest",
    "test.watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ionic-team/stencil-ds-plugins.git"
  },
  "author": "Ionic Team",
  "homepage": "https://stenciljs.com/",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ionic-team/stencil-ds-plugins/issues"
  },
  "devDependencies": {
    "@angular/core": "^7.2.15",
    "@angular/forms": "^7.2.15",
    "@types/jest": "^24.0.11",
    "jest": "^24.5.0",
    "np": "^4.0.2",
    "rimraf": "^2.6.3",
    "rollup": "^1.6.0",
    "tslint": "^5.14.0",
    "tslint-ionic-rules": "0.0.21",
    "typescript": "^3.3.3333"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|ts|tsx)$": "<rootDir>/test/jest.preprocessor.js"
    },
    "testRegex": "(\\.(test|spec))\\.(ts?|tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ],
    "testURL": "http://localhost"
  },
  "gitHead": "bbdf59a588a3afd0e75c24081aa3d9030be49a62"
}
