{
  "name": "component-library-react",
  "sideEffects": false,
  "version": "0.0.4",
  "private": true,
  "description": "React specific wrapper for component-library",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/stencil-ds-output-targets.git"
  },
  "scripts": {
    "test": "jest",
    "build": "npm run clean && npm run compile",
