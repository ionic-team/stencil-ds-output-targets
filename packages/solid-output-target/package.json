{
  "name": "@stencil/solid-output-target",
  "version": "0.0.1",
  "description": "Solid output target for @stencil/core components.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "solid-component-lib/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build": "tsc && npm run rollup",
    "watch": "tsc --watch",
    "rollup": "rollup -c",
    "version": "npm run build",
    "release": "np",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ionic-team/stencil-ds-output-targets.git"
  },
  "author": "Ionic Team",
  "contributors": [
    {
      "name": "Martin Oppitz",
      "email": "github@martinoppitz.com",
      "url": "https://github.com/deleonio"
    }
  ],
  "homepage": "https://stenciljs.com/",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ionic-team/stencil-ds-output-targets/issues"
  },
  "peerDependencies": {
    "@stencil/core": ">=1.8.0"
  },
  "devDependencies": {
    "solid-js": "^1.1.4"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|ts|tsx)$": "<rootDir>/test/jest.preprocessor.js"
    },
    "testRegex": "(\\.(test|spec))\\.(ts?|tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ],
    "testURL": "http://localhost"
  }
}
